function BuyItem(v)
game:GetService("ReplicatedStorage"):WaitForChild("BuyItemEvent"):FireServer(v)
end
game:GetService("RunService").RenderStepped:Connect(function()
BuyItem("SmallSphere")
BuyItem("Square")
BuyItem("Tulip")
BuyItem("SmallCube")
BuyItem("Platform")
BuyItem("Wedge")
BuyItem("Tree")
BuyItem("Flower")
BuyItem("Wall")
BuyItem("SmallCylinder")
BuyItem("BasicJumps")
BuyItem("Crate")
BuyItem("MicroCube")
BuyItem("BigCylinder")
BuyItem("BigSphere")
BuyItem("Truss")
BuyItem("BallJumps")
BuyItem("Fence")
BuyItem("DamageBrick")
BuyItem("Ice")
BuyItem("RainbowTrusses")
BuyItem("Water")
BuyItem("KillBrick")
BuyItem("Disappearing")
BuyItem("Teleporter")
BuyItem("SpiralStairs")
BuyItem("Refiner")
BuyItem("WallHops")
BuyItem("PaintBucket")
BuyItem("SpeedGiver")
BuyItem("SlapGiver")
BuyItem("TrollPart")
end)
