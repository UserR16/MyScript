
function BuyItem(v)
if game:GetService("Players").LocalPlayer.PlayerGui.HUD["Shop-GUI"]["Shop-Container"].ScrollingFrame[v].Buttons.CashBuy.BackgroundColor3 == Color3.fromRGB(25, 202, 16) then
game:GetService("ReplicatedStorage"):WaitForChild("BuyItemEvent"):FireServer(v)
end
end

game:GetService("RunService").Stepped:Connect(function()
if _G.AutoBuyItems then
BuyItem("SmallSphere")
BuyItem("Square")
BuyItem("Tulip")
BuyItem("SmallCube")
BuyItem("Platform")
BuyItem("Wedge")
BuyItem("Tree")
BuyItem("Flower")
BuyItem("Wall")
BuyItem("SmallCylinder")
BuyItem("BasicJumps")
BuyItem("Crate")
BuyItem("MicroCube")
BuyItem("BigCylinder")
BuyItem("BigSphere")
BuyItem("Truss")
BuyItem("BallJumps")
BuyItem("Fence")
BuyItem("DamageBrick")
BuyItem("Ice")
BuyItem("RainbowTrusses")
BuyItem("Water")
BuyItem("KillBrick")
BuyItem("Disappearing")
BuyItem("Teleporter")
BuyItem("SpiralStairs")
BuyItem("Refiner")
BuyItem("WallHops")
BuyItem("PaintBucket")
BuyItem("SpeedGiver")
BuyItem("SlapGiver")
BuyItem("TrollPart")
end
end)
