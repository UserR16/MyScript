for _,v in ipairs(workspace.GameAssets.Debris.Cleanable:FindFirstChild("BananaPeelAbility"):GetChildren()) do
if v.Parent.Name == "BananaPeelAbility" and v:IsA("BasePart") then
v.CanTouch = not _G.AntiBanana
end
end
end
end)
