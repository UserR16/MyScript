local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/liebertsx/Tora-Library/main/src/librarynew",true))()
local Window1 = Library:CreateWindow("BLOXLOOT")
local Event = Window1:AddFolder("Raid Boss")
Event:AddToggle({
	text = "Auto King Skeleton",
	callback = function(v)
_G.FarmKingSkeleton = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.FarmKingSkeleton and workspace.World1.EnemySpawnPools["1"]:FindFirstChild("1_2_100"):FindFirstChild("1_2_100").Torso then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.World1.EnemySpawnPools["1"]["1_2_100"]["1_2_100"].Torso.CFrame
end
end)
end})
Event:AddToggle({
	text = "Auto Spider Queen",
	callback = function(v)
_G.FarmSpiderQueen = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.FarmSpiderQueen and workspace.World1.EnemySpawnPools["1"]:FindFirstChild("1_2_101"):FindFirstChild("1_2_101").Torso then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.World1.EnemySpawnPools["1"]["1_2_101"]["1_2_101"].Torso.CFrame
end
end)
end})
--[[local Farm = Window1:AddFolder("Farm")
local function farm_mod(mod, part)
if workspace.World1[part].EnemySpawnPoints:FindFirstChild(mod):FindFirstChildOfClass("Model") then
for _,v in ipairs(workspace.World1[part].EnemySpawnPoints:GetChildren()) do
if v.Name == mod and v:FindFirstChildOfClass("Model"):FindFirstChild("HumanoidRootPart") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChildOfClass("Model").HumanoidRootPart.CFrame
elseif v.Name == mod and v:FindFirstChildOfClass("Model"):FindFirstChild("RootPart") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChildOfClass("Model").RootPart.CFrame
end
end
end
end
Farm:AddList({
    text = "Select Mod",
    values = {"Chicken", "Pig", "Llama", "Zombie", "Creeper", "Bat", "Slime", "Copper Zombie", "Skeleton"},
    callback = function(v)
_G.SelectModFarm = v
end})
Farm:AddToggle({
	text = "Farm Mod",
	callback = function(v)
_G.FarmMod = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.FarmMod then
if _G.SelectModFarm == "Chicken" then
farm_mod("1_1_1", "Part1")
elseif _G.SelectModFarm == "Pig" then
farm_mod("1_1_2", "Part1")
elseif _G.SelectModFarm == "Llama" then
farm_mod("1_1_4", "Part1")
elseif _G.SelectModFarm == "Zombie" then
farm_mod("1_1_5", "Part1")
elseif _G.SelectModFarm == "Creeper" then
farm_mod("1_1_6", "Part1")
elseif _G.SelectModFarm == "Bat" then
farm_mod("1_2_1", "Part2")
elseif _G.SelectModFarm == "Slime" then
farm_mod("1_2_2", "Part2")
elseif _G.SelectModFarm == "Copper Zombie" then
farm_mod("1_2_3", "Part2")
elseif _G.SelectModFarm == "Skeleton" then
farm_mod("1_2_4", "Part2")
end
